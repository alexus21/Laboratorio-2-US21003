<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageViewDeleteCategory"
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toEndOf="@+id/imageViewEditCategory"
        app:layout_constraintTop_toBottomOf="@+id/textViewCategoryName"
        app:srcCompat="@drawable/recycle_bin_line_icon"
        app:tint="@android:color/holo_red_dark" />

    <ImageView
        android:id="@+id/imageViewEditCategory"
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textViewCategoryName"
        app:srcCompat="@drawable/pencil_icon"
        app:tint="@android:color/holo_green_light"
        tools:layout_editor_absoluteX="16dp" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:tint="#00CC92"
        app:srcCompat="@drawable/online_survey_icon" />

    <TextView
        android:id="@+id/textViewCategoryName"
        android:layout_width="300dp"
        android:layout_height="46dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.26"
        app:layout_constraintStart_toEndOf="@+id/imageView21"
        tools:layout_editor_absoluteY="20dp" />
</androidx.constraintlayout.widget.ConstraintLayout>